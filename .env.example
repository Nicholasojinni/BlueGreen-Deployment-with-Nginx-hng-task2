# .env.example - Copy to .env and fill before running
BLUE_IMAGE=docker pull yimikaade/wonderful:devops-stage-two
GREEN_IMAGE=docker pull yimikaade/wonderful:devops-stage-two

# Which pool is active on nginx startup: "blue" or "green"
ACTIVE_POOL=blue

# The release id strings to pass to the apps so they report it in X-Release-Id
RELEASE_ID_BLUE=blue-v1
RELEASE_ID_GREEN=green-v1

# Optional override of ports used by compose (not usually changed)
PORT=8080
BLUE_PORT=8081
GREEN_PORT=8082
